{
    "presets": [
        [
            "@babel/preset-env",
            {
                "targets": {
                    "node": "4"
                }
            }
        ]
    ],
    "plugins": [
        [
            "@babel/plugin-transform-runtime",
            {
                "corejs": false,
                "helpers": true,
                "regenerator": true,
                "useESModules": false
            }
        ]
    ]
}